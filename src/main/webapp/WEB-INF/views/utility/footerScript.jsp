 <script type="text/javascript">		
	$(document).ready(function() {
		$.ajax({
			url: "${pageContext.request.contextPath}/customer/customerAutocomplateSearch",
			method: "get",
			dataType: "json",
			success: function(data){
				var options = {
						data : data,
						getValue: function(element) {
							full_name = element.customer_name;
							return full_name;
						},
						template : {
							type : "description",
							fields : {
								description : "customer_mobile"
							}
						},
						list : {
							match: {
								enabled: true
							},
							onClickEvent : function(){
								var value = $("#square").getSelectedItemData().customer_id;
								location.href = "${pageContext.request.contextPath}/customer/profile/"+value;
							},
							onKeyEnterEvent : function(){
								var value = $("#square").getSelectedItemData().customer_id;
								location.href = "${pageContext.request.contextPath}/customer/profile/"+value;
							},
						}
				}
				$("#square").easyAutocomplete(options);
			}
		});
		
		
	});
</script>

